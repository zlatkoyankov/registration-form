    "jquery": "2.2.3",
    "lodash": "^4.16.4",
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-addons-css-transition-group": "15.3.2",
    "validator": "^6.0.0",
    "webpack": "1.13.0",
    "expect": "^1.19.0"
